<Alloy>
    <Window title="People & Places">
        <TableView>
            <TableViewRow leftImage="/images/icons/peoplesearch.png" onClick="peopleSearchClick">
                <Label>People Search</Label>
            </TableViewRow>
            <TableViewRow leftImage="/images/icons/deptsearch.png" onClick="deptSearchClick">
                <Label>Department Search</Label>
            </TableViewRow>
            <TableViewRow leftImage="/images/icons/location.png" onClick="locationSearchClick">
                <Label>Location Search</Label>
            </TableViewRow>
            <TableViewRow leftImage="/images/icons/map.png" onClick="campusMapClick">
                <Label>Campus Map</Label>
            </TableViewRow>
            <TableViewRow leftImage="/images/icons/parking.png" onClick="parkingMapClick">
                <Label>Parking Map</Label>
            </TableViewRow>
        </TableView>
    </Window>
</Alloy>