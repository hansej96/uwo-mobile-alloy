<Alloy>
    <Window title="Student Recreation & Wellness Center">
        <TableView>
            <TableViewRow class="orange"><Label class="header">Student Recreation \u0026 Wellness Center</Label></TableViewRow>
            <TableViewRow onClick="newsClick"><Label class="left">Tap for News</Label></TableViewRow>
            <TableViewRow />
            <TableViewRow class="gray"><Label class="header left">SRWC Hours</Label></TableViewRow>
            <TableViewRow><Label class="left">Sunday</Label><Label class="right">10 a.m. - Midnight</Label></TableViewRow>
            <TableViewRow><Label class="left">Monday - Thursday</Label><Label class="right">6 a.m. - Midnight</Label></TableViewRow>
            <TableViewRow><Label class="left">Friday</Label><Label class="right">6 a.m. - 11 p.m.</Label></TableViewRow>
            <TableViewRow><Label class="left">Saturday</Label><Label class="right">10 a.m. - 11 p.m.</Label></TableViewRow>
            <TableViewRow />
            <TableViewRow onClick="membershipClick"><Label class="left">Tap for Membership Information</Label></TableViewRow>
            <TableViewRow onClick="featuresClick"><Label class="left">Tap for Features</Label></TableViewRow>
            <TableViewRow onClick="policiesClick"><Label class="left">Tap for Policies</Label></TableViewRow>
            <TableViewRow onClick="parkingClick"><Label class="left">Tap for Parking</Label></TableViewRow>
        </TableView>
    </Window>
</Alloy>
